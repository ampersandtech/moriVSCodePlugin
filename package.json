{
  "name": "morivscodeplugin",
  "displayName": "moriVSCodePlugin",
  "description": "usefull plugin for programmers at mori",
  "version": "0.0.3",
  "publisher": "moridevelopment",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mori.importAndRequire",
    "onCommand:mori.copyrightHeader"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [{
      "command": "mori.copyrightHeader",
      "key": "ctrl+shift+c",
      "mac": "ctrl+shift+c",
      "when": "editorTextFocus"
    },{
      "command": "mori.importAndRequire",
      "key": "alt+shift+p",
      "mac": "ctrl+shift+p",
      "when": "editorTextFocus"
    }],
    "commands": [
      {
        "command": "mori.importAndRequire",
        "title": "Mori Import / Require"
      },
      {
        "command": "mori.copyrightHeader",
        "title": "Mori Copyright Header"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "moment": "^2.18.1"
  }
}
